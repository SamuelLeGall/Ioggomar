{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "node",
    "importHelpers": true,
    "jsx": "preserve",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "sourceMap": true,
    "baseUrl": "./",
    "paths": {
      "@assets/*": [
        "ioggomarApp/src/assets/*"
      ],
      "@composables/*": [
        "ioggomarApp/src/vuejs/app/composables/*"
      ],
      "@components/*": [
        "ioggomarApp/src/vuejs/app/components/*"
      ],
      "@config/*": [
        "ioggomarApp/src/config/*"
      ],
      "@localizations/*": [
        "ioggomarApp/src/localizations/*"
      ],
      "@models/*": [
        "ioggomarApp/src/models/*"
      ],
      "@router/*": [
        "ioggomarApp/src/vuejs/app/router/*"
      ],
      "@stores/*": [
        "ioggomarApp/src/vuejs/app/stores/*"
      ],
      "@styles/*": [
        "ioggomarApp/src/styles/*"
      ],
      "@utils/*": [
        "ioggomarApp/src/utils/*"
      ],
    },
    "strict": true,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "types": ["vitest/globals"]
  },
  "include": [
    "ioggomarApp"
, "ioggomarApp/src/vuejs/vitest.config.ts"  ],
  "references": [
    {
      "path": "./tsconfig.node.json"
    }
  ]
}